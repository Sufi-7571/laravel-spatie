{
    "name": "voltic/product-reviews",
    "description": "Product reviews and ratings package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sufiyan Mirza",
            "email": "sufimiz.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Voltic\\ProductReviews\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Voltic\\ProductReviews\\ProductReviewsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}